{
  /*接口使用的参数*/
  "porterParams": [
    /*传给服务器的:responsURI，为响应uri*/

    /*扫描路径*/
    {
      "name": "webPorterPaths",
      "value": [
        {
          "isSys": false,//默认为false
          "porters": [
            "com.chenyg.myclass.porters",
            "com.chenyg.users.porters"
          ]
        }
      ]
    },
    /*前缀*/
    {
      "name": "tiedPrefix",
      "value": ""
    },
    /*设置状态监听器,必须有无参构造函数，监听器被调用的顺序与添加顺序对应*/
    {
      "name": "stateListeners",
      "value": []
    },
    /*设置字符编码*/
    {
      "name": "encoding",
      "value": "utf-8"
    },
    /*资源未找到的ThinkType，默认为DEFAULT*/
    {
      "name": "notFoundThinkType",
      "value": "DEFAULT"
    },
    /*设置全局检测对象*/
    {
      "name": "globleCheck",
      "value": "com.xxx.xxx.MyGlobleCheck"
    },
    /*设置是否返回参数错误报告，默认为false。
            例如：当某个请求缺乏必须参数时，若设置为true，则返回缺乏的具体必须参数名；否则不返回；
                {NeedArgs:[...]}*/
    {
      "name": "responseLosedArgs",
      "value": "true"
    },
    /*默认检测类型*/
    {
      "name": "defaulCheckType",
      "value": "GLOBAL"
    },
    /*设置服务器请求错误监听*/
    {
      "name": "exListener",
      "value": "com.xxx.xxx.MyRequestExListener"
    },
    {
      "name": "wpLog",
      "value": "com.chenyg.wporter.log.WPLogSys"
    }
  ],
  /*用户自定义参数*/
  "userParams": [
  ]
}
